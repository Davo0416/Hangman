body.hangman{
	background-image:url('Images/background/HangmanBG.png');
	background-repeat:repeat; 
	  background-size: 400px 400px;    
}

p.inline{
	font-weight: 800;
	display: inline-block;
	width: 7%;
	margin: 0px;
	font-size: 28px;
	padding: 2px;
	text-align: center;
}

p.heart{
	display: inline-block;
	width: 7%;
	color: red;
	text-shadow: 1px 2px 0 FireBrick;
	margin: 0px;
	font-size: 40px;
	padding: 2px;
}

p.tag{
	font-size: 15px;
	margin: 0px;
	background-color: #ebc028;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	text-align: center;
	width: 50%;
	margin-left: auto; 
	margin-right: auto;
	box-shadow: 0 5px 0 0 #917b2c ,0 10px 9px 6px #c7ba8d;
	font-weight: 800;
	color: #fff;
	text-shadow: 0 1px 0 rgba(0,0,0,0.5);
}

input[type=text].input {
	
	background-color:LightYellow;
	height:50px;
	outline:none;
	border-radius: 5px;
	border: 5px solid #ebc028;
	font-size: 33px;
	box-shadow: 0 5px 0 0 #917b2c ,0 10px 9px 6px #c7ba8d;
	font-weight: 600;
	padding: 0px 20px;
	width:100%;
}

p.yellow{
	background-color:LightYellow;
	height:50px;
	width:relative;
	border-radius: 5px;
	border: 5px solid #ebc028;
	font-size: 33px;
	box-shadow: 0 5px 0 0 #917b2c ,0 10px 9px 6px #c7ba8d;
	font-weight: 600;
	margin: 0px;
	padding-left: 10px;

}

h1{
	display: inline-block;
	font-size: 53px;
	text-shadow: 1px 2px 0 rgba(0,0,0,0.2);
	margin: 0px;
	font-weight: 800;
	text-align:center;
}

button{
	outline:none;
    border: 0;
    border-radius: 15px;
    box-shadow: 0 5px 0 0 #917b2c ,0 10px 9px 6px #c7ba8d;
    display: inline-block;
    text-align: center;
    color: #fff;
    font-size: 30px;
    font-weight: 800;
    text-shadow: 0 2px 0 rgba(0,0,0,0.5);
    width:relative;
    background-color: #ebc028;
}

button:hover{	
	background-color: #deb326;
}


button:active{
	background-color: #edc742;
}



div.hangman{
	background-color: PaleGoldenRod; 
	padding: 10px;
	border-radius: 7px;
    width: 23%; 
    border: 10px solid #ebc028;
    padding: 7px;
	margin: 5px;
	margin-left: 50px;
	display:inline-block;

}

div.inline{
	display:inline-block;
	width: 200px;
}
div.inline2{
	display:inline-block;
