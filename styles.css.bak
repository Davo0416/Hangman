*{
	padding:0px;
	margin:0px;
	text-decoration:none;
	list-style:none;
	box-sizing:border-box;
}

a.active,a:hover{
	background: #46a9eb;
	transition: 0.5s;
}

a{
	background: #1b9bff;
}

nav{
	
	background:#0082e6;
	height:80px;
	width:100%;
	margin-bottom: 25px;
}
label.logo{
	color:white;
	font-size:35px;
	line-height:80px;
	padding:0 100px 0 0;
	font-weight:bold;
	margin-left:40px;
}

ul{
	display:inline-block;
	height: 5px;
	padding-top: 50px;
}

nav ul{
	float:right;
	margin:0px;
}
nav ul li{
	display:inline-block;
	margin:0 5px;
}
nav ul li a{
	color:white;
	font-size:17px;
	padding:7px 13px;
	border-radius:3px;
	text-transform:uppercase;
}


.bubble {
  position: relative;
  font-family: sans-serif;
  font-size: 18px;
  line-height: 24px;
  width: max-content;
  background: #fff;
  border-radius: 40px;
  padding: 24px;
  text-align: center;
  outline:none;
  color: #000;

}

.bubble-bottom-left:before {
  content: "";
  width: 0px;
  height: 0px;
  position: absolute;
  border-left: 24px solid #fff;
  border-right: 12px solid transparent;
  border-top: 12px solid #fff;
  border-bottom: 20px solid transparent;
  left: 32px;
  bottom: -24px;
}

